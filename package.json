{
  "name": "boundless-sdk-build",
  "private": true,
  "version": "1.0.9",
  "description": "SDK for OpenLayers 3 and React plus Flux",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/boundlessgeo/sdk.git"
  },
  "scripts": {
    "build:babel": "cross-env NODE_ENV=release babel ./src --out-dir ./release",
    "start": "npm-run-all --parallel build:copy:css babel watch:css watch:scss",
    "generate:css": "node scripts/generate-css.js",
    "generate:sdk:css": "node-sass sdk.scss > sdk.css",
    "postprocess:css": "npm-run-all generate:sdk:css generate:css",
    "watch:css": "onchange 'src/components/*.css' -- npm run build:copy:css",
    "watch:scss": "onchange 'src/components/*.scss' -- npm run build:css",
    "babel": "babel --watch ./src --out-dir ./release",
    "build:copy-files": "babel-node ./scripts/copy-files.js",
    "build:copy:css": "cp src/components/*.css release/components",
    "createdir:release:locale": "mkdirp release/locale",
    "babel:locale": "cross-env NODE_ENV=release babel locale/en.js -o release/locale/en.js",
    "build:copy:locale": "npm-run-all createdir:locale i18n createdir:release:locale babel:locale",
    "copy:dist:release": "cp -R dist release",
    "build:copy:dist": "npm-run-all build copy:dist:release",
    "clean:build": "rimraf release",
    "release": "npm-run-all clean:build build:babel build:copy-files build:css build:copy:locale build:copy:dist",
    "cover": "mochify --extension .jsx --plugin [ mochify-istanbul --report text --report lcovonly --dir ./coverage --exclude **/src/constants/* --exclude **/src/**/*.css --exclude **/src/**/*.scss --exclude **/__tests__/**/* ] --transform ./scripts/test-transform.js __tests__/**/*.test.js",
    "createdir:build:js": "mkdirp dist/js",
    "build:scss": "node-sass ./src/components/ -o ./src/components",
    "build:css": "npm-run-all build:scss build:copy:css postprocess:css ",
    "removedir:css": "rimraf dist/css",
    "createdir:css": "mkdirp dist/css",
    "createdir:build:css": "npm-run-all removedir:css createdir:css",
    "createdir:docs": "mkdirp docs/api",
    "createdir:locale": "mkdirp locale",
    "postinstall": "node scripts/install.js",
    "i18n": "node scripts/generate-locale.js",
    "css:drfrankenstyle": "dr-frankenstyle dist/css",
    "minify:css": "cleancss -o dist/css/components.min.css dist/css/components.css",
    "css": "npm-run-all css:drfrankenstyle postprocess:css minify:css",
    "build:full:debug": "browserify --extension .jsx -p licensify -g [envify --NODE_ENV development] src/full.js -o dist/js/full-debug.js",
    "build:full": "browserify --extension .jsx -p licensify -g [envify --NODE_ENV production] -p [minifyify --no-map --exclude node_modules/openlayers/dist/ol.js]  src/full.js -o dist/js/full.js",
    "build": "npm-run-all createdir:build:js createdir:build:css css build:full build:full:debug",
    "generate:docs": "node scripts/generate-api-doc.js",
    "generate:gitbook": "gitbook build",
    "generate:pdf": "gitbook pdf",
    "docs": "npm-run-all createdir:docs generate:docs generate:gitbook generate:pdf",
    "serve:gitbook": "gitbook serve",
    "test:mochify": "mochify --extension .jsx --reporter list --transform ./scripts/test-transform.js __tests__/**/*.test.js",
    "test:lint": "eslint __tests__ src --ext .jsx,.js",
    "test": "npm-run-all build:scss test:mochify test:lint",
    "copy:build": "cp -R dist out",
    "copy:docs": "cp -R _book out/book",
    "copy:mapskinFont": "cp -R node_modules/mapskin/fonts release/fonts",
    "spec": "npm-run-all createdir:build:js createdir:build:css createdir:docs build generate:docs generate:gitbook copy:build copy:docs"
  },
  "author": "Boundless Spatial, Inc.",
  "license": "Apache-2.0",
  "engines": {
    "node": ">6"
  },
  "peerDependencies": {
    "react": "15.3.2",
    "react-dom": "15.3.2",
    "react-tap-event-plugin": "^1.0.0"
  },
  "dependencies": {
    "blueimp-canvas-to-blob": "2.2.2",
    "browser-filesaver": "^1.1.0",
    "c3-windows": "^0.4.11-rc4",
    "classnames": "^2.2.5",
    "debounce": "^1.0.0",
    "file-saver": "^1.3.3",
    "flux": "^2.0.1",
    "hint.css": "^2.4.0",
    "jison": "^0.4.17",
    "@boundlessgeo/jsonix": "^2.4.3",
    "jspdf": "^1.3.2",
    "keymirror": "~0.1.0",
    "lodash.uniqueid": "^3.0.0",
    "mapskin": "^1.0.0",
    "material-ui": "0.16.4",
    "moment": "^2.13.0",
    "nuka-carousel": "git://github.com/FormidableLabs/nuka-carousel.git#v2.1.1",
    "ogc-schemas": "^2.6.0",
    "ol3-cesium": "3.9.4",
    "openlayers": "4.2.0",
    "react-color": "2.8.0",
    "react-dnd": "^2.3.0",
    "react-dnd-html5-backend": "^2.1.2",
    "react-dnd-text-dragpreview": "^0.2.3",
    "react-dropzone": "3.6.0",
    "react-intl": "^2.1.5",
    "react-responsive-decorator": "0.0.1",
    "react-table": "^5.6.0",
    "reactcss": "^1.1.1",
    "url-parse": "^1.1.1",
    "w3c-schemas": "^1.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^4.1.6",
    "babel-plugin-react-intl": "2.2.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.19.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-1": "^6.16.0",
    "babel-runtime": "^6.11.6",
    "babelify": "^7.3.0",
    "browserify": "13.1.0",
    "chai": "^1.10.0",
    "chai-xml": "^0.3.1",
    "clean-css": "^3.4.6",
    "coveralls": "^2.11.14",
    "cross-env": "^3.2.3",
    "cssify": "^0.8.0",
    "dr-frankenstyle-win": "0.2.13",
    "envify": "3.4.0",
    "es6-promise": "^3.0.2",
    "eslint": "1.9.0",
    "eslint-plugin-react": "3.9.0",
    "gitbook-cli": "2.1.2",
    "intl": "^1.0.0",
    "licensify": "^2.2.0",
    "minifyify": "^7.1.0",
    "mkdirp": "^0.5.1",
    "mocha": "^2.1.0",
    "mocha-lcov-reporter": "^1.2.0",
    "mochify": "^2.18.1",
    "mochify-istanbul": "^2.4.1",
    "node-sass": "^4.5.0",
    "npm-run-all": "^1.2.11",
    "onchange": "^3.2.1",
    "phantomjs-polyfill-find": "github:ptim/phantomjs-polyfill-find",
    "phantomjs-polyfill-object-assign": "0.0.2",
    "phantomjs-prebuilt": "^2.1.7",
    "raf": "^3.2.0",
    "react": "15.3.2",
    "react-addons-test-utils": "15.3.2",
    "react-docgen": "^2.2.0",
    "react-dom": "15.3.2",
    "react-tap-event-plugin": "^1.0.0",
    "rimraf": "^2.5.3",
    "sinon": "^1.12.2",
    "through": "^2.3.8",
    "watchify": "^3.2.0"
  },
  "browserify": {
    "transform": [
      "cssify",
      "babelify"
    ]
  }
}
